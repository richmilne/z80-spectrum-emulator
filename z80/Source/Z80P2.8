rreg_cb:
        cmp bh,64
        ja rreg_findjxf
        mov al,bh
        and al,7
        cmp al,6
        jz rregcb_next
rreg_findjxf:
        inc bx
        cmp w[bx],03d8a
        jne rreg_findjxf
        cmp w[bx+2],0e3ff
        jne rreg_findjxf
        mov ax,bx
        stosw
        mov al,bh
        and al,7
        cmp al,6                ;(hl)
        mov al,8
        jnz rreg_putt
        mov al,15
        cmp bh,64
        jb rreg_putt
        cmp bh,128
        jae rreg_putt
        mov al,12               ;bit n,(hl)
rreg_putt:
        stosb
rregcb_next:
        mov bl,02c
        inc bh
        jne rreg_cb
        xor ax,ax
        stosw
        stosb
        pop ds
        ret


